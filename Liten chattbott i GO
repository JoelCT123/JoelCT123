#En liten chattbott som kommer ih√•g vad man svarar byggd i GO

package main

import (
  "fmt"
)

func main() {

var name string
fmt.Println ("What is your name?")
fmt.Scan (&name)
fmt.Println ("Hello,", name)

var age int
fmt.Println("How old are you?")
fmt.Scan (&age)

fmt.Printf("Hello %s you are %d years old", name, age)
}
